{
  "name": "@quority/fandom",
  "version": "4.0.9",
  "description": "Typed MediaWiki API client for node.js using TypeScript.",
  "main": "dist/main.js",
  "files": [
    "dist/**",
    "!dist/demo/**",
    "!dist/tests/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "types": "./dist/main.d.ts"
    }
  },
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "yarn tsc --listEmittedFiles --incremental",
    "re-build": "rimraf tsconfig.tsbuildinfo dist && yarn tsc --listEmittedFiles --incremental",
    "lint": "eslint src/ --ext .ts --cache",
    "lint:fix": "eslint src/ --fix --ext .ts --cache",
    "hooks": "husky install .github/husky",
    "test": "mocha ./dist/tests/**/*.js --timeout 120000"
  },
  "keywords": [
    "Fandom",
    "MediaWiki",
    "MediaWiki API",
    "wiki"
  ],
  "author": "bitomic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quority/fandom.git"
  },
  "homepage": "https://github.com/quority/fandom#readme",
  "devDependencies": {
    "@atlaskit/adf-schema": "^25.2.1",
    "@atlaskit/adf-utils": "^18.0.3",
    "@quority/core": "^4.0.3",
    "@types/form-data": "2.5.0",
    "@types/fs-extra": "11.0.1",
    "@types/mocha": "10.0.0",
    "@types/node": "18.11.9",
    "@types/tough-cookie": "4.0.2",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.27.0",
    "husky": "8.0.2",
    "lint-staged": "^13.0.3",
    "mocha": "10.1.0",
    "rimraf": "3.0.2",
    "ts-dotenv": "^0.9.0",
    "tsup": "^6.5.0",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.8.6",
  "dependencies": {
    "@sapphire/shapeshift": "^3.8.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@quority/core": "*"
  }
}
